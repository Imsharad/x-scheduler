# Twitter/X API credentials (OAuth 1.0a)
X_API_KEY=your_api_key_here
X_API_KEY_SECRET=your_api_secret_here
X_ACCESS_TOKEN=your_access_token_here
X_ACCESS_TOKEN_SECRET=your_access_token_secret_here
X_BEARER_TOKEN=your_bearer_token_here

# OAuth 2.0 credentials for video upload
X_OAUTH_CLIENT_ID=your_oauth_client_id_here
X_OAUTH_CLIENT_SECRET=your_oauth_client_secret_here
X_OAUTH_REDIRECT_URI=http://localhost:5000/callback
